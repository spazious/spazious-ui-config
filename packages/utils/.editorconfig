# Editor configuration, see http://editorconfig.org
# This file is for unifying the coding style for different editors and IDEs
# when working with the project.
# More information at https://github.com/spazious/spazious-ui-config
# Do not edit this file, is automatically generated and liked by the
# @spazious/prettier-config package
root = true

[*]
charset = utf-8
insert_final_newline = true
end_of_line = lf
indent_style = space
indent_size = 2
max_line_length = 80
trim_trailing_whitespace = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
