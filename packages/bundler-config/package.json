{
  "name": "@spazious/bundler-config",
  "version": "0.0.1",
  "description": "Bundler config for Spazious",
  "type": "commonjs",
  "main": "./dist/vite.mjs",
  "types": "./dist/vite.d.ts",
  "bin": {
    "spz-bundler-config": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "scripts": {
    "dev": "yarn build --watch --sourcemap",
    "build": "tsup-node src/index.ts src/vite.ts --dts --format cjs,esm",
    "test": "exit 0"
  },
  "keywords": [
    "tsup",
    "vite",
    "config",
    "frontend",
    "react"
  ],
  "resolutions": {
    "graphql": "^16"
  },
  "dependencies": {
    "@spazious/vite-config": "^0.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.7",
    "tsup": "^6.7.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "@spazious/eslint-config": "^0.0.1"
  },
  "author": "Rubén López Gómez (http://rubeniskov.com) <ruben.lopez@spazious.com>",
  "contributors": [
    {
      "name": "Rubén López Gómez",
      "email": "ruben.lopez@spazious.com",
      "url": "https://rubeniskov.com"
    }
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/spazious/spazious-ui-config/issues"
  },
  "homepage": "https://github.com/spazious/spazious-ui-config/packages/vite-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/spazious/spazious-ui-config.git"
  }
}
